	"version": "0.2.0",
	"configurations": [
		{
			"name": "boil -v",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v"
			],
		},
		{
			"name": "boil help",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-h"
			]
		},
		{
			"name": "boil exec",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"-r=${workspaceFolder}/_testdata/testrepo",
				"exec",
				"-w",
				"apps/cliapp#all",
				"--target-dir=${workspaceFolder}/_testdata/output",
				"--var=ProjectName=Penis",
				"--var=ModulePath=github.com/vedranvuk/cliapp"
			]
		},
		{
			"name": "boil list",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"list"
			]
		},
		{
			"name": "boil snap",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"cwd": "${workspaceFolder}/_testdata/snapshot",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"snap",
				"snapshot",
				"-w"
			]
		},
		{
			"name": "boil snap -z",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}/_testdata/snapshot",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"snap",
				"snapshot",
				"-w",
				"-z"
			]
		},
		{
			"name": "boil new test",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"console": "integratedTerminal",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"new",
				"test"
			]
		},
		{
			"name": "boil new test/test",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"console": "integratedTerminal",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"new",
				"test/test"
			]
		},
		{
			"name": "boil edit snapshot prompts",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"console": "integratedTerminal",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"edit",
				"snapshot",
				"prompts"
			]
		},
		{
			"name": "boil edit test all",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"console": "integratedTerminal",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"edit",
				"test",
				"all"
			]
		},
		{
			"name": "boil edit go/cmd prompts",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"console": "integratedTerminal",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"edit",
				"go/cmd",
				"prompts"
			]
		},
	]
}