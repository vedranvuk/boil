{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "boil",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"--config=${workspaceFolder}/cmd/boil/config.json",
				"--repository=${workspaceFolder}/_testdata/templates"
			],
		},
		{
			"name": "boil help",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-h"
			]
		},
		{
			"name": "boil exec",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"-r=${workspaceFolder}/_testdata/testrepo",
				"exec",
				"-w",
				"apps/cliapp#all",
				"--target-dir=${workspaceFolder}/_testdata/output",
				"--var=ProjectName=Penis",
				"--var=ModulePath=github.com/vedranvuk/cliapp"
			]
		},
		{
			"name": "boil list",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"list"
			]
		},
		{
			"name": "boil snap",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"cwd": "${workspaceFolder}/_testdata/snapshot",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"snap",
				"snapshot",
				"-w"
			]
		},
		{
			"name": "boil snap -z",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}/_testdata/snapshot",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"-v",
				"snap",
				"snapshot",
				"-w",
				"-z"
			]
		},
		{
			"name": "boil new test",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/boil",
			"args": [
				"new",
				"test"
			]
		}
	]
}